<ul class='gallery'>
  {{#each this}}
    <li class='gallery-item js-photo'>
      <div class='photo-card'>
        <a href='{{largeImageURL}}' target='_blank'>
          <img src='{{webformatURL}}' alt='{{tags}}' width='480' />
        </a>
        <div class='stats'>
          <p class='stats-item'>
            <i class='material-icons'>thumb_up</i>
            {{likes}}
          </p>
          <p class='stats-item'>
            <i class='material-icons'>visibility</i>
            {{views}}
          </p>
          <p class='stats-item'>
            <i class='material-icons'>comment</i>
            {{comments}}
          </p>
          <p class='stats-item'>
            <i class='material-icons'>cloud_download</i>
            {{downloads}}
          </p>
        </div>
      </div>
    </li>
  {{/each}}
</ul>